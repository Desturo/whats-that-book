{
	"schemaVersion": 1,
	"id": "wtb",
	"version": "${version}",
	"name": "What's That Book?",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Desturo"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/wtb/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.desturo.wtb.WhatsThatBook"
		],
		"client": [
			"net.desturo.wtb.WhatsThatBookClient"
		],
		"fabric-datagen": [
			"net.desturo.wtb.ExampleModDataGenerator"
		]
	},
	"mixins": [
		"wtb.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}